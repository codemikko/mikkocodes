{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "preview",
          "name": "preview",
          "type": "string"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "datetime"
        },
        {
          "title": "tools",
          "name": "tools",
          "type": "choice",
          "choices": [
            "vsCode",
            "Github",
            "Gist"
          ]
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "other",
  "frontMatter.content.publicFolder": "",
  "frontMatter.content.pageFolders": [
    {
      "title": "authors",
      "path": "[[workspace]]/content/authors",
      "originalPath": "[[workspace]]/content/authors"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts",
      "originalPath": "[[workspace]]/content/posts"
    },
    {
      "title": "projects",
      "path": "[[workspace]]/content/projects"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "HTML",
    "CSS",
    "JavaScript",
    "React",
    "Vue",
    "Angular",
    "Bootstrap",
    "jQuery",
    "Webpack",
    "Node.js",
    "Express",
    "MongoDB",
    "GraphQL",
    "REST API",
    "Authentication",
    "Authorization",
    "Testing",
    "Deployment"
  ],
  "frontMatter.taxonomy.categories": [
    "Frontend",
    "Backend",
    "Full-stack",
    "Database",
    "API",
    "Authentication",
    "Authorization",
    "Testing",
    "Deployment"
  ],
  "frontMatter.taxonomy.tools": [
    "VS Code",
    "Git",
    "GitHub",
    "GitLab",
    "Bitbucket",
    "Gist",
    "Postman",
    "Docker",
    "Jenkins",
    "Travis CI",
    "CircleCI",
    "Heroku"
  ]
}